<p id="notice"><%= notice %></p>

<h1>Listing Voucher Particulars</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @voucher_particulars.each do |voucher_particular| %>
      <tr>
        <td><%= link_to 'Show', voucher_particular %></td>
        <td><%= link_to 'Edit', edit_voucher_particular_path(voucher_particular) %></td>
        <td><%= link_to 'Destroy', voucher_particular, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

